# Root .gitignore

# --- General / OS ---
.DS_Store
Thumbs.db
*.log
logs/
.tmp/
tmp/

# Local env files and secrets
.env
.env.*
*.env.local
*.secret

# --- Editors / IDE ---
.idea/
.vscode/

# --- Python / Django ---
__pycache__/
*.py[cod]
*.pyo
*.pyd
*.so

# Virtual environments
.venv/
venv/
env/

# Packaging / tooling caches
.pytest_cache/
.mypy_cache/
.ruff_cache/
.tox/
.cache/

# Local SQLite DBs
db.sqlite3
db.sqlite3-journal

# PostgreSQL
*.sql
*.dump
*.pgsql

# Django collectstatic output (commit your source assets, not the collected ones)
staticfiles/
static_root/

# Userâ€‘uploaded media (served from MEDIA_ROOT)
media/

# Coverage
.coverage
coverage.xml
htmlcov/

# --- JavaScript / React ---
node_modules/
